<Page
    x:Class="CustomPropertyProviderApp.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:CustomPropertyProviderApp"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    mc:Ignorable="d">

    <StackPanel>
        <TextBlock Text="{Binding Guid, Mode=OneWay}" />
        <Button Click="Button_Click" Content="Create a new CustomPropertyProvider ViewModel" />
        <Button Click="Button_Click2" Content="Create a new ViewModel" />
    </StackPanel>
</Page>
